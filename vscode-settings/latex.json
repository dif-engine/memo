		"body" : [
			"\\\\begin{tikzpicture}",
			"$1",
			"\\\\end{tikzpicture}"
		],
		"description": "tikzpicture"
	},

	"expression (tagless)": {
		"prefix" : "ex",
		"body" : ["\\$$1\\$"],
		"description": "tagless expression"
	},

	"expression (tagless, displaystyle)": {
		"prefix" : "exd",
		"body" : ["\\$\\\\displaystyle $1\\$"],
		"description": "tagless expression (display style)"
	},

