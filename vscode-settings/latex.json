{
	"comment": {
		"prefix" : "comment",
		"body" : [
			"\\\\begin{comment}",
			"$1",
			"\\\\end{comment}\n"
		],
		"description": "latex comment"
	},

	"mindflow": {
		"prefix" : "mflow",
		"body" : [
			"\\\\begin{mindflow}",
			"$1",
			"\\\\end{mindflow}\n"
		],
		"description": "mindflow"
	},

  	"textbf": {
		"prefix" : "bf",
		"body" : [
			"\\\\textbf{$1}$2"
		],
		"description" : "textbf"
	},

	"texttt": {
		"prefix" : "tt",
		"body" : [
			"\\\\texttt{$1}$2"
		],
		"description": "texttt"
	},

	"textit": {
